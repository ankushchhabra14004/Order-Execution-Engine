{
  "info": {
    "name": "Order Execution Engine",
    "_postman_id": "mock-collection",
    "description": "Example POST to execute an order",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Execute Market Order",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"type\": \"market\",\n  \"tokenIn\": \"A\",\n  \"tokenOut\": \"B\",\n  \"amountIn\": 100\n}"
        },
        "url": {
          "raw": "http://localhost:3000/api/orders/execute",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["api","orders","execute"]
        }
      }
    }
  ]
}
